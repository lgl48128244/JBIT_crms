<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.accp.crm.pojo.Power" table="POWER" schema="CRM">
        <id name="powerid" type="java.lang.String">
            <column name="POWERID" length="100" />
            <generator class="assigned" />
        </id>
        <property name="powerdesc" type="java.lang.String">
            <column name="POWERDESC" length="500" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="URL" length="100" />
        </property>
    </class>
</hibernate-mapping>
