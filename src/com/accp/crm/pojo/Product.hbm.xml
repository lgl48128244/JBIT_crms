<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.accp.crm.pojo.Product" table="Product" schema="CRM">
        <id name="id" type="java.lang.Integer">
            <column name="ID"/>
            <generator class="increment" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" length="50" />
        </property>
        <property name="typeNo" type="java.lang.String">
            <column name="TYPENO" length="50" />
        </property>
        <property name="grades" type="java.lang.String">
            <column name="GRADES" length="20" />
        </property>
        <property name="batch" type="java.lang.String">
            <column name="BATCH" length="20" />
        </property>
        <property name="unit" type="java.lang.String">
            <column name="UNIT" length="20" />
        </property>
        <property name="price" type="java.lang.Integer">
            <column name="PRICE" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="50" />
        </property>
    </class>
</hibernate-mapping>
