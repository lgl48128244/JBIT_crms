<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.accp.crm.pojo.Linkman" table="LINKMAN" schema="CRM">
        <id name="lid" type="java.lang.Integer">
            <column name="ID" precision="22" scale="0" />
            <generator class="increment" />
        </id>
        <property name="custno" type="java.lang.String">
            <column name="CUSTNO" length="50" />
        </property>
        <property name="custname" type="java.lang.String">
            <column name="CUSTNAME" length="100" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="NAME" length="20" />
        </property>
        <property name="sex" type="java.lang.String">
            <column name="SEX" length="6" />
        </property>
        <property name="position" type="java.lang.String">
            <column name="POSITION" length="20" />
        </property>
        <property name="tel" type="java.lang.String">
            <column name="TEL" length="30" />
        </property>
        <property name="mobile" type="java.lang.String">
            <column name="MOBILE" length="30" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="REMARK" length="100" />
        </property>
    </class>
</hibernate-mapping>
