<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.accp.crm.pojo.CustInfo" table="CUSTINFO" schema="CRM">
        <id name="id" type="java.lang.Integer">
            <column name="ID"/>
            <generator class="increment" />
        </id>
        <property name="custno" type="java.lang.String">
            <column name="CUSTNO" length="50" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="NAME" length="100" />
        </property>
        <property name="region" type="java.lang.String">
            <column name="REGION" length="50" />
        </property>
        <property name="custManager" type="java.lang.String">
            <column name="CUST_MANAGER" length="50" />
        </property>
        <property name="levelLabel" type="java.lang.String">
            <column name="LEVELLABEL" length="50" />
        </property>
        <property name="satisfy" type="java.lang.String">
            <column name="SATISFY" length="30" />
        </property>
        <property name="credit" type="java.lang.String">
            <column name="CREDIT" length="30"/>
        </property>
        <property name="custAddr" type="java.lang.String">
            <column name="CUST_ADDR" length="200" />
        </property>
        <property name="zip" type="java.lang.String">
            <column name="ZIP" length="10" />
        </property>
        <property name="tel" type="java.lang.String">
            <column name="TEL" length="50" />
        </property>
        <property name="fax" type="java.lang.String">
            <column name="FAX" length="50" />
        </property>
        <property name="website" type="java.lang.String">
            <column name="WEBSITE" length="100" />
        </property>
        <property name="licence" type="java.lang.String">
            <column name="LICENCE" length="50" />
        </property>
        <property name="chieftain" type="java.lang.String">
            <column name="CHIEFTAIN" length="50" />
        </property>
        <property name="bankroll" type="java.lang.Integer">
            <column name="BANKROLL" precision="22" scale="0" />
        </property>
        <property name="turnover" type="java.lang.Integer">
            <column name="TURNOVER" precision="22" scale="0" />
        </property>
        <property name="bank" type="java.lang.String">
            <column name="BANK" length="200" />
        </property>
        <property name="account" type="java.lang.String">
            <column name="ACCOUNT" length="50" />
        </property>
        <property name="localTax" type="java.lang.String">
            <column name="LOCAL_TAX" length="50" />
        </property>
        <property name="nationalTax" type="java.lang.String">
            <column name="NATIONAL_TAX" length="50" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="STATUS" precision="22" scale="0" />
        </property>
    </class>
</hibernate-mapping>
