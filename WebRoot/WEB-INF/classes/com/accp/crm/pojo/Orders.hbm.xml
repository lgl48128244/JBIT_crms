<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.accp.crm.pojo.Orders" table="ORDERS" schema="CRM">
        <id name="oid" type="java.lang.Integer">
            <column name="OID" precision="22" scale="0" />
            <generator class="increment" />
        </id>
        <property name="custno" type="java.lang.String">
            <column name="CUSTNO" length="50" />
        </property>
        <property name="custname" type="java.lang.String">
            <column name="CUSTNAME" length="100" />
        </property>
        <property name="odate" type="java.lang.String">
            <column name="ODATE" length="50" />
        </property>
        <property name="address" type="java.lang.String">
            <column name="ADDRESS" length="200" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="STATUS" precision="22" scale="0" />
        </property>
    </class>
</hibernate-mapping>
