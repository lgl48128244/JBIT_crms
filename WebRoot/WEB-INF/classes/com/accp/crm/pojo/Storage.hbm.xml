<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.accp.crm.pojo.Storage" table="Storage" schema="CRM">
        <id name="id" type="java.lang.Integer">
            <column name="ID"/>
            <generator class="increment" />
        </id>
        <property name="prod_id" type="java.lang.String">
            <column name="prod_id" />
        </property>
        <property name="prod_name" type="java.lang.String">
            <column name="prod_name" length="50"/>
        </property>
        <property name="ware_house" type="java.lang.String">
            <column name="ware_house" length="50" />
        </property>
        <property name="ware" type="java.lang.String">
            <column name="ware" length="50" />
        </property>
        <property name="count" type="java.lang.Integer">
            <column name="count" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="200" />
        </property>
    </class>
</hibernate-mapping>
